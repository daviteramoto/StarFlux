@model List<TorreViewModel>

<table class="table table-striped">
    <tr>
        <th>Ações</th>
        <th>Código</th>
        <th>Nome</th>
    </tr>

    @foreach (TorreViewModel torre in Model)
    {
        <tr>
            <td>
                <a href="/torre/edit?id=@torre.ID"><img width="20" src="~/img/edit.png" /></a>
                <a href="javascript:apagarTorre(@torre.ID)"><img width="20" src="~/img/delete.png" /></a>
            </td>
            <td>@torre.ID</td>
            <td>@torre.Nome</td>
        </tr>
    }
</table>